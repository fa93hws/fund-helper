{
  "compilerOptions": {
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2018",
    "baseUrl": "./",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "services/*": ["src/services/*"],
      "utils/*": ["src/utils/*"]
    }
  }
}
